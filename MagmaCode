iload "magma_lecture9.m";
Qx<x> := PolynomialRing(Rationals());
f:= x^2+1;
K := NumberField(f);
Qy<y> := PolynomialRing(K);                        
g := y^5+1;
C:= HyperellipticCurve(g);              
J := Jacobian(C);
TorsionBound(J,5);

